#################################################################
Arduino nano setting

Kart: "Arduino Nano" or "Arduino Mini"
İşlemci: "ATmega168"
Port: "/dev/ttyUSB0"
Kart Bilgisi Al

Programlayıcı: "Arduino as ISP"
Önyükleyiciyi Yazdır
#################################################################
  // HC-SR04 SPECS
  digitalWrite(trigPin, HIGH);
  delayMicroseconds(10);
  digitalWrite(trigPin, LOW);
  // Duration return value for Arduino nano 6200 per meter nearly
  duration = pulseIn(echoPin, HIGH); // 6200 per meter
  delay(60); // 60ms delay look at HC-SR04 datasheet
  
 #################################################################


